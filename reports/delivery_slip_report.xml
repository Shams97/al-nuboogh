<odoo>
    <data>
        <template id="stock_delivery_slip_report_nuboogh" inherit_id="stock.report_delivery_document">
            <xpath expr="//span[@t-field='move.product_uom_qty']" position="replace">
                <span t-esc="int(move.product_uom_qty)"/>
            </xpath>
            <xpath expr="//span[@t-field='bo_line.product_uom_qty']" position="replace">
                <span t-esc="int(bo_line.product_uom_qty)"/>
            </xpath>
        </template>

        <template id="stock_delivery_slip_report_nuboogh_2" inherit_id="stock.stock_report_delivery_aggregated_move_lines">
            <xpath expr="//td[@name='move_line_aggregated_qty_done']/span[1]" position="replace">
                <span t-esc="int(aggregated_lines[line]['qty_done'])"/>
            </xpath>
        </template>

        <template id="stock_delivery_slip_report_nuboogh_3" inherit_id="stock.stock_report_delivery_has_serial_move_line">
            <xpath expr="//span[@t-field='move_line.qty_done']" position="replace">
                <span t-esc="int(move_line.qty_done)"/>
            </xpath>
        </template>

    </data>
</odoo>